{% for item in list %}

    <tr data-id="{{ item.rent_req_id }}">
        <td style="text-align: center" class="data_id">{{ item.rent_req_id }}</td>
        <td style="text-align: center"><a href="{{ global.base_url }}/manager/taxi/rent_requests/edit/{{ item.rent_req_id  }}/">{{ item.rent_req_name }}</a></td>
        <td style="text-align: center">{{ item.rent_req_phone }}</td>
        <td style="text-align: center"><a href="{{ global.base_url }}/manager/taxi/cars/edit/{{ item.rent_req_car_id }}">{{ item.car_title }}</a></td>
        <td style="text-align: center">{{ item.rent_req_time }}</td>
        <td>
            <a target="_blank" href="{{ global.base_url }}/manager/taxi/rent_requests/edit/{{ item.rent_req_id  }}/" class="btn btn-primary btn-xs"><i class="fa fa-pencil"></i></a>
            <div data-value="{{ item.rent_req_id }}" class="btn btn-danger btn-xs delete"><i class="fa"></i></div>
        </td>

    </tr>
{% endfor %}
